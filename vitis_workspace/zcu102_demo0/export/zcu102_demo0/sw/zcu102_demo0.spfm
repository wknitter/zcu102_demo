<?xml version="1.0" encoding="UTF-8"?>
<sdx:platform sdx:vendor="xilinx.com"
              sdx:library="sdx"
              sdx:name="zcu102_demo0"
              sdx:version="1.0"
              sdx:schemaVersion="1.0"
              xmlns:sdx="http://www.xilinx.com/sdx">
    <sdx:description>
zcu102_demo0
    </sdx:description>
    <sdx:systemConfigurations sdx:defaultConfiguration="zcu102_demo0">
        <sdx:configuration sdx:name="zcu102_demo0"
                           sdx:displayName="zcu102_demo0"
                           sdx:defaultProcessorGroup="standalone_domain"
                           sdx:runtimes="cpp">
            <sdx:description>zcu102_demo0</sdx:description>
            <sdx:bootImages sdx:default="standard">
                <sdx:image sdx:name="standard"
                            sdx:bif="zcu102_demo0/boot/zcu102_demo0.bif"
                            sdx:readme="zcu102_demo0/boot/generic.readme"
                            sdx:qemuArguments="zcu102_demo0/qemu/pmu_args.txt:zcu102_demo0/qemu/qemu_args.txt"
                            />
                <sdx:image sdx:name="standalone_domain"
                            sdx:bif="zcu102_demo0/boot/zcu102_demo0.bif"
                            sdx:readme="zcu102_demo0/boot/generic.readme"
                            sdx:qemuArguments="zcu102_demo0/standalone_domain/qemu/pmu_args.txt:zcu102_demo0/standalone_domain/qemu/qemu_args.txt"
                            />
            </sdx:bootImages>
            <sdx:processorGroup sdx:name="standalone_domain"
